<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="j05combobox.ComboBoxDemoController">
   <children>
      <Label text="Please choose one from the combo box">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="image" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <ComboBox fx:id="comboBox" prefWidth="150.0" />
         </children>
      </HBox>
      <Button fx:id="chooseButton" mnemonicParsing="false" onAction="#handleChooseButton" text="Choose" />
      <Separator prefHeight="34.0" prefWidth="600.0" />
      <Label fx:id="result" text="Label" />
   </children>
</VBox>
